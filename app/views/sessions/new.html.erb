<div class = "login">
<h2>GA Search Engine</h2>
<h3 class = "hire"> Hire. Get Hired.</h3>

<%= form_tag session_path, :method => :post do %>
  <%= text_field_tag :email, nil, :placeholder => "Email" %>

  <%= password_field_tag :password, nil, :placeholder => "Password" %>

  <%= submit_tag "Login" %>
<% end %>
</div>
