<%= form_tag search_students_path, method: "GET" do %>

  <% inputs = ["Product Manager","Entrepreneur","Digital Marketer", "Founder/Co-Founder", "UX Designer", "Product Designer", "Full-Stack Developer", "Back-end Developer", "Front-end Developer"] %>
    <% inputs.each do |input| %>
      <%= check_box_tag input.to_sym %>
      <%= label_tag input.to_sym %>
    <% end %> 
    
  <%= submit_tag "Search" %>

 <% end %> 
